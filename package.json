{
  "name": "vuetify-google-autocomplete",
  "version": "2.0.0-Alpha.8",
  "description": "A Vuetify ready Vue.jS component to Google Places Autocomplete",
  "main": "dist/index.js",
  "homepage": "https://github.com/MadimetjaShika/vuetify-google-autocomplete",
  "repository": {
    "type": "git",
    "url": "https://github.com/MadimetjaShika/vuetify-google-autocomplete.git"
  },
  "keywords": [
    "vue",
    "vuetify",
    "google",
    "autocomplete"
  ],
  "scripts": {
    "dev": "npm run build && cross-env NODE_ENV=development webpack --progress --hide-modules",
    "build": "mkdir -p ./dist && babel ./src/ -d ./dist",
    "minify": "minify ./dist/ -d ./dist",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot",
    "production": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "deploy:example": "gh-pages -d example/dist",
    "test": "jest --coverage  --no-cache --forceExit"
  },
  "files": [
    "dist"
  ],
  "author": "Madi Shika <mmjshika@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-minify": "^0.3.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-env": "^3.2.5",
    "cross-env": "^5.1.3",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.13.0",
    "gh-pages": "^1.1.0",
    "jest": "^22.4.2",
    "jest-serializer-vue": "^0.3.0",
    "jest-sonar-reporter": "^1.3.0",
    "jest-vue-preprocessor": "^1.3.1",
    "laravel-mix": "^0.8.8",
    "vue": "^2.5.13",
    "vue-clipboards": "^1.2.1",
    "vue-jest": "^2.1.0",
    "vue-node": "^1.1.1",
    "vue-test-utils": "^1.0.0-beta.11",
    "vuetify": "^1.0",
    "webpack": "^3.11.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./jest-reports",
    "clearMocks": true,
    "notify": true,
    "verbose": true,
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  }
}
